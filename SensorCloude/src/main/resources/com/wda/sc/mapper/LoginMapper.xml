<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.hellomx.mapper.CustomerMapper">

	<select id="login" resultType="com.wda.sc.domain.memberVO">
		select * from game where id = #{id}
	</select>
	
	<insert id="signup" parameterType="com.wda.sc.domain.memberVO">
		insert into MEMBER(user_id,name,password,phone,email) values(#{userid},#{name},#{password},#{phone},#{email})
	</insert>
	
</mapper>